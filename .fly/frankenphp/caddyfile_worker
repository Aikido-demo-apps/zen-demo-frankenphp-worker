:8080 {

    root * /var/www/html/public

    php_server {
        env DOCUMENT_ROOT "/var/www/html/public"
        env AIKIDO_TOKEN ${AIKIDO_TOKEN}
        worker {
            file /var/www/html/public/index.php
            num 5
        }
    }

    file_server
}
